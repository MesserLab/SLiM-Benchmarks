/*
	This exercises rpois() for the lambda non-singleton case
*/

initialize() { }
1 early() {
	lambda = runif(10000000, 9.5, 10.5);
	
	start = clock("mono");
	for (i in 1:10)
		x = rpois(10000000, lambda);
	catn("// ********** Benchmark time: " + (clock("mono") - start));

	// summarize results
	catn("### NO SUMMARY ###");
}
