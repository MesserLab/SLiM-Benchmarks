/*
	A simple nonneutral WF model with lots of mutations.  This exercises UpdateFitness(),
	UniqueMutationRuns(), and TallyMutationReferences_FAST() in addition to
	DoCrossOverMutation().
	
*/

initialize() {
	initializeMutationRate(1e-5);	// temporary
	initializeMutationType("m1", 0.5, "f", -0.0001);
	initializeGenomicElementType("g1", m1, 1.0);
	initializeGenomicElement(g1, 0, 1e7-1);
	initializeRecombinationRate(1e-8);
}
1 early() { sim.addSubpop("p1", 100); }
10 early() {
	sim.chromosome.setMutationRate(1e-7);
	p1.setSubpopulationSize(100000);
}

25 late() {
	// summarize results
	catn("### NO SUMMARY ###");
}
