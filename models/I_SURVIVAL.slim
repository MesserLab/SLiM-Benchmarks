/*
	This exercises nonWF deferred reproduction with typical parameters
*/

initialize() {
	// This model uses an internal benchmark
	_startBenchmark("SURVIVAL");
	
	initializeSLiMModelType("nonWF");
	defineConstant("K", 10000);
}
reproduction() {
	subpop.addCrossed(individual, subpop.sampleIndividuals(1), count=10);
}
1 early() {
	sim.addSubpop("p1", K);
}
early() {
	p1.fitnessScaling = K / p1.individualCount;
}
5000 late() {
	//catn("elapsed: " + _stopBenchmark());

	// summarize results
	catn("### NO SUMMARY ###");
}
