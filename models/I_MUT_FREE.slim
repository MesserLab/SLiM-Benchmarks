/*
	A simple nonWF model designed to exercise mutation freeing after tallying.
*/

initialize() {
	_startBenchmark("MUT_FREE");
	
	initializeMutationRate(1e-4);
	initializeMutationType("m1", 0.5, "f", 0.0);
	initializeGenomicElementType("g1", m1, 1.0);
	initializeGenomicElement(g1, 0, 1e7 - 1);
	initializeRecombinationRate(0.0);
}
1 early() {
	sim.addSubpop("p1", 5);
}
2000 late() {
	//catn("elapsed: " + _stopBenchmark());

	// summarize results
	catn("### NO SUMMARY ###");
}
