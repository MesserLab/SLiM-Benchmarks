/*
	This exercises Eidos function round(float x)
*/

initialize() { }
1 early() {
	x = runif(10000000, -100, 100);
	o = round(x);	// we only do a consistency check
	
	start = clock("mono");
	for (i in 1:3000)
		y = round(x);
	catn("// ********** Benchmark time: " + (clock("mono") - start));
	
	// test only the last one, to avoid overhead
	if (identical(o, y))
		catn("### CORRECT ###");
	else
		catn("### *** INCORRECT *** ###");
}

