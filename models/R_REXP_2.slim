/*
	This exercises rexp() for the mu non-singleton case
*/

initialize() { }
1 early() {
	mu = runif(10000000, 9.5, 10.5);
	
	start = clock("mono");
	for (i in 1:50)
		x = rexp(10000000, mu);
	catn("// ********** Benchmark time: " + (clock("mono") - start));

	// summarize results
	catn("### NO SUMMARY ###");
}
