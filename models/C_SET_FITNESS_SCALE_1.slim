/*
	This exercises setting Individual property fitnessScaling to a singleton value
*/

initialize() { }
1 early() {
	sim.addSubpop("p1", 1000000);
	inds = p1.individuals;
	
	start = clock("mono");
	for (i in 1:2000) inds.fitnessScaling = 1.1;
	catn("// ********** Benchmark time: " + (clock("mono") - start));
	
	// summarize results
	catn("### NO SUMMARY ###");
}
