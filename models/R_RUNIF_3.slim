/*
	This exercises runif() for the no singleton case
*/

initialize() { }
1 early() {
	min = runif(10000000, 0.0, 0.1);
	max = runif(10000000, 1.0, 1.1);
	
	start = clock("mono");
	for (i in 1:100)
		x = runif(10000000, min, max);
	catn("// ********** Benchmark time: " + (clock("mono") - start));
}
