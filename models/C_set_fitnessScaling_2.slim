/*
	C_set_fitnessScaling_2.slim
	
	This exercises setting Individual property fitnessScaling to a vector
*/

initialize() { }
1 early() {
	sim.addSubpop("p1", 1000000);
	inds = p1.individuals;
	v = asFloat(seqAlong(inds));
	
	start = clock("mono");
	for (i in 1:2000) inds.fitnessScaling = v;
	catn("TEST TIME: " + (clock("mono") - start));
	
	// summarize results
	catn("### NO SUMMARY ###");
}
